<%- include('../partials/header') %>

<section id="show-page">
    <p>Class Type: <%= classes.classType %></p> 
    <p>Instructor: <%= classes.instructor %></p> 
    <p>Level: <%= classes.level %></p> 
    <p>Date: <%= classes.date %></p> 
    <p>Duration: <%= classes.duration %></p> 
  </section>
  
  <br/><br/>

  <h2 id="registered">Registered Members</h2>
  <table id="registeredMember">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% classes.registration.forEach(function(r) { %>
      <tr>
       <td><%= r.name %></td>
      <td><%= r.email %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>